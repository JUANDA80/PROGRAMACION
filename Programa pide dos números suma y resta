#include <stdio.h>
#include <stdlib.h>

int main () 
{
	int num1,num2,suma,resta;
	
	printf("Dame el primer numero: \n");
	scanf("%d",&num1);
	printf("Dame el segundo numero: \n");
	scanf("%d",&num2);
	
	suma=num1+num2;
	printf("La suma es:%d \n",suma);
	
	resta=num1-num2;
	printf("La resta es:%d",resta);
	return 0;
}
